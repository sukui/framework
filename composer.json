    },
    "1": {
      "type": "git",
      "url": "https://github.com/sukui/http-client.git"
    },
    "2": {
      "type": "git",
      "url": "https://github.com/sukui/database.git"
    },
    "4": {
      "type": "git",
      "url": "https://github.com/sukui/routing.git"
    }
  },
  "require": {
    "php": ">=5.6",
    "psr/log": "1.0.0",
    "zanphp/internal-sdk": "dev-master",
    "zan-config/zan": "dev-master",
    "zanphp/cache": "dev-master",
    "zanphp/exception": "dev-master",
    "zanphp/coroutine": "dev-laogui",
    "zanphp/timer": "dev-master",
    "zanphp/container": "dev-master",
    "zanphp/contracts": "dev-master",
    "zanphp/loadbalance": "dev-master",
    "zanphp/service-chain": "dev-laogui",
    "zanphp/spi": "dev-master",
    "zanphp/config": "dev-master",
    "zanphp/support": "dev-master",
    "zanphp/hawk": "dev-master",
    "zanphp/console": "dev-master",
    "zanphp/dns-client": "dev-master",
    "zanphp/database": "dev-laogui",
    "zanphp/debugger": "dev-master",
    "zanphp/connection-pool": "dev-laogui",
    "zanphp/trace": "dev-laogui",
    "zanphp/log": "dev-laogui",
    "zanphp/nosql": "dev-master",
    "zanphp/rpc-context": "dev-master",
    "zanphp/http-client": "dev-laogui",
    "zanphp/http-foundation": "dev-laogui",
    "zanphp/routing": "dev-laogui",
    "zanphp/service-store": "dev-master",
    "zanphp/utilities": "dev-laogui",
    "zanphp/nova": "dev-laogui",
    "zanphp/nova-codec": "dev-master",
    "zanphp/nova-generic": "dev-laogui",
    "zanphp/nova-connection-pool": "dev-master",
    "zanphp/etcd-client": "dev-laogui",
    "zanphp/etcd-registry": "dev-laogui",
    "zanphp/validation": "dev-master",
    "zanphp/testing": "dev-laogui",
    "zanphp/worker-monitor": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "ZanPHP\\Framework\\": "src/"
    },
    "files": [
      "src/Foundation/Application.php",
      "src/Contract/Foundation/Bootable.php"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "preferred-install": "dist",
    "secure-http": false
  },
  "minimum-stability": "dev"
}